package com.sosks1313.dto;

import java.sql.Timestamp;

public class BDto {
	
	int bId;
	String bName;
	String bTitle;
	String bContent;
	Timestamp bDate;  //timestamp = 특정 시각을 나타내는 문자열, 이떄는 글 작성 시간에 쓰인다.
	int bHit;
	int bGroup;
	int bStep;
	int bIndent;
	
	public BDto(int bId,
				String bName,
				String bTitle,
				String bContent,
				Timestamp bDate,
				int bHit,
				int bGroup,
				int bStep,
				int bIndent) {
		super();
		this.bId = bId;
		this.bName = bName;
		this.bTitle = bTitle;
		this.bContent = bContent;
		this.bDate = bDate;
		this.bHit = bHit;
		this.bGroup = bGroup;
		this.bStep = bStep;
		this.bIndent = bIndent;
		
	}//생성자
	
	public int getbId() {
		return bId;
	}
	
	public void setbId(int bId) {
		this.bId = bId;
	}
	
	public String getbName() {
		return bName;
	}
	
	public void setbName() {
		this.bName = bName;
	}
	
	public String getbTitle() {
		return bTitle;
	}
	
	public void setbTitle() {
		this.bTitle = bTitle;
	}
	
	public String getbContent() {
		return bContent;
	}
	
	public void setbContent() {
		this.bContent = bContent;
	}
	
	public Timestamp getbDate() {
		return bDate;
	}
	
	public void setbDate() {
		this.bDate = bDate;
	}
	
	public int bHit() {
		return bHit;
	}
	
	public void setbHit() {
		this.bHit = bHit;
	}
	
	public int getbGroup() {
		return bGroup;
	}
	
	public void setbGroup() {
		this.bGroup = bGroup;
	}
	
	public int getbStep() {
		return bStep;
	}
	
	public void setbStep() {
		this.bStep = bStep;
	}
	
	public int getbIndent() {
		return bIndent;
	}
	
	public void setbIndent(int bIndent) {
		this.bIndent = bIndent;
	}
	
	
}
